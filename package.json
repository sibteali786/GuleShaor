{
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "gsap": "^3.10.4",
    "mongoose": "^6.4.6"
  },
  "name": "guleshaor",
  "version": "1.0.0",
  "description": "A Social media App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build":"cd guleshaor-frontend && npm run build",
    "install-guleshaor-frontend":"cd guleshaor-frontend && npm install",
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false npm run install-guleshaor-frontend && npm run buil",
    "start": "node guleshaor-backend/server.js",
    "server": "nodemon guleshaor-backend/server.js",
    "client": "npm start --prefix guleshaor-frontend",
    "dev": "concurrently \"npm run server\" \"npm run client \"",
    "data:import": "node guleshaor-backend/seeder",
    "data:destroy": "node guleshaor-backend/seeder -d"
  },
  "author": "GuleShaor",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19"
  }
}
